[MESSAGES CONTROL]
disable=missing-module-docstring, relative-beyond-top-level
ignore-mixin-members=yes

[BASIC]
no-docstring-rgx=__.*__|_.*
class-rgx=[A-Z_][a-zA-Z0-9_]+$
class-attribute-rgx=([A-Za-z_][A-Za-z0-9_]{2,40}|(__.*__))$$
function-rgx=[a-z_][a-zA-Z0-9_]{2,70}$
method-rgx=[a-z_][a-zA-Z0-9_]{2,70}$
attr-rgx=[a-z_][a-zA-Z0-9_]{2,70}$
const-rgx=(([A-Z_][A-Z0-9_]*){2,70}|(__.*__))$
good-names=_,f,i,j,k,v,x,y,z,e,qs,id,pk,ok,r,g,b,a,n

[DESIGN]
max-args=10
max-attributes=10
min-public-methods=0
max-public-methods=100
max-parents=20
max-locals=25
max-branches=20